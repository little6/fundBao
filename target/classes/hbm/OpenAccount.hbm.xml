<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.fundBao.domain.OpenAccountVo" table="ac_trade_account">
		<id name="tradeAccountNo" column="trade_account_no" type="integer">
			<generator class="increment" />
		</id>
		<property name="name" column="name" type="string" />
		<property name="certType" column="cert_type" type="string" />
		<property name="certNo" column="cert_no" type="string" />

		<property name="agentName" column="agent_name" type="string" />
		<property name="agentCertType" column="agent_cert_type" type="string" />
		<property name="agentCertNo" column="agent_cert_no" type="string" />

		<property name="attnName" column="attn_name" type="string" />
		<property name="attnCertType" column="attn_cert_type" type="string" />
		<property name="attnCertNo" column="attn_cert_no" type="string" />

		<property name="legalPersonName" column="legal_person_name"
			type="string" />
		<property name="legalPersonCertType" column="legal_person_cert_type"
			type="string" />
		<property name="legalPersonCertNo" column="legal_person_cert_no"
			type="string" />
	</class>
</hibernate-mapping>    